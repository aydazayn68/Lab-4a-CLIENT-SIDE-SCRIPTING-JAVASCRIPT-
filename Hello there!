</head>
<body>
  <div id="myElement">
    <p>Welcome to my website!</p>
  </div>

  <script>
    // Get a reference to the paragraph element
    const paragraph = document.querySelector("#myElement p");

    // Change the text content of the paragraph
    paragraph.textContent = "Hello there!";
  </script>
</body>
